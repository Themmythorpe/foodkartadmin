\n  methods: {\r\n    setImage(e) {\r\n      const file = e.target.files[0];\r\n      if (!file.type.includes(\"image/\")) {\r\n        alert(\"Please select an image file\");\r\n        return;\r\n      }\r\n      if (typeof FileReader === \"function\") {\r\n        const reader = new FileReader();\r\n        reader.onload = event => {\r\n          this.imgSrc = event.target.result;\r\n          // rebuild cropperjs with the updated source\r\n          this.$refs.cropper.replace(event.target.result);\r\n        };\r\n        reader.readAsDataURL(file);\r\n      } else {\r\n        alert(\"Sorry, FileReader API not supported\");\r\n      }\r\n    },\r\n    cropImage() {\r\n      // get image data for post processing, e.g. upload or setting image src\r\n      this.cropImg = this.$refs.cropper.getCroppedCanvas().toDataURL();\r\n    },\r\n    rotate() {\r\n      // guess what this does :)\r\n      this.$refs.cropper.rotate(90);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":";;;;AAwEA,OAAAA,UAAA;AACA;AAEA;EACAC,UAAA;IACAD,UAAA,EAAAA;EACA;EACAE,QAAA;IACA;IACAC,KAAA;EACA;EAEAC,IAAA,WAAAA,KAAA;IACA;MACAC,MAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,QAAA,WAAAA,SAAAC,CAAA;MAAA,IAAAC,KAAA;MACA,IAAAC,IAAA,GAAAF,CAAA,CAAAG,MAAA,CAAAC,KAAA;MACA,KAAAF,IAAA,CAAAG,IAAA,CAAAC,QAAA;QACAC,KAAA;QACA;MACA;MACA,WAAAC,UAAA;QACA,IAAAC,MAAA,OAAAD,UAAA;QACAC,MAAA,CAAAC,MAAA,aAAAC,KAAA;UACAV,KAAA,CAAAL,MAAA,GAAAe,KAAA,CAAAR,MAAA,CAAAS,MAAA;UACA;UACAX,KAAA,CAAAY,KAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAJ,KAAA,CAAAR,MAAA,CAAAS,MAAA;QACA;QACAH,MAAA,CAAAO,aAAA,CAAAd,IAAA;MACA;QACAK,KAAA;MACA;IACA;IACAU,SAAA,WAAAA,UAAA;MACA;MACA,KAAApB,OAAA,QAAAgB,KAAA,CAAAC,OAAA,CAAAI,gBAAA,GAAAC,SAAA;IACA;IACAC,MAAA,WAAAA,OAAA;MACA;MACA,KAAAP,KAAA,CAAAC,OAAA,CAAAM,MAAA;IACA;EACA;AACA"}]}